{
  "name": "capture-it-ext",
  "description": "A browser extension to Capture & Edit Screenshots on the go!",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "node ./scripts/patterns/build.js && wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "git-remove": "bun run ./scripts/git-helper.js remove-git",
    "git-change": "bun run ./scripts/git-helper.js change-repo",
    "git-show": "bun run ./scripts/git-helper.js show-git",
    "git": "bun run ./scripts/git-helper.js add-git",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@ant-design/cssinjs": "^2.0.1",
    "@fontsource/poppins": "^5.2.7",
    "@tailwindcss/vite": "^4.1.17",
    "@types/lodash": "^4.17.21",
    "@webext-core/messaging": "^2.3.0",
    "@wxt-dev/i18n": "^0.2.4",
    "antd": "^6.0.1",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "color": "^5.0.3",
    "disable-devtool": "^0.3.9",
    "html-to-image": "^1.11.13",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-easy-crop": "^5.5.6",
    "react-router-dom": "^7.10.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "vite-plugin-remove-console": "^2.2.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.32",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-react": "^1.1.5",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "wxt": "^0.20.13"
  }
}
