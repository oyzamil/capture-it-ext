{
  "name": "capture-it-ext",
  "description": "A browser extension to take beautiful screenshots.",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "git-remove": "bun run ./scripts/git-helper.js remove-git",
    "git-change": "bun run ./scripts/git-helper.js change-repo",
    "git-show": "bun run ./scripts/git-helper.js show-git",
    "git-add": "bun run ./scripts/git-helper.js add-git",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@ant-design/cssinjs": "^2.0.1",
    "@ant-design/icons": "6.1.0",
    "@fontsource/poppins": "^5.2.7",
    "@heroicons/react": "^2.2.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@supabase/supabase-js": "^2.86.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/lodash": "^4.17.21",
    "@wxt-dev/i18n": "^0.2.4",
    "antd": "^6.0.1",
    "arrive": "^2.5.2",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "color": "^5.0.3",
    "dom-to-image-more": "^3.7.2",
    "html-to-image": "^1.11.13",
    "html2canvas-pro": "^1.6.1",
    "lodash": "^4.17.21",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router-dom": "^7.10.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "vite-plugin-remove-console": "^2.2.0",
    "webext-bridge": "^6.0.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/arrive": "^2.4.4",
    "@types/chrome": "^0.1.32",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-react": "^1.1.5",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "wxt": "^0.20.11"
  },
  "overrides": {
    "@wxt-dev/browser": "^0.1.4"
  }
}
